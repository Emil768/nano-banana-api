PORT=3000
NODE_ENV=production

# Frontend URL (where generate page is hosted)
FRONTEND_ORIGIN=https://nanobananaa.ru
FRONTEND_SUCCESS_REDIRECT=https://nanobananaa.ru/generate.html
FRONTEND_ERROR_REDIRECT=https://nanobananaa.ru/generate.html?auth_error=1

# Telegram
TELEGRAM_BOT_TOKEN=123456:replace_me
TELEGRAM_WIDGET_MAX_AGE_SECONDS=300

# Cookies
COOKIE_DOMAIN=.nanobananaa.ru
COOKIE_SECURE=true
COOKIE_SAMESITE=Lax
COOKIE_MAX_AGE_SECONDS=2592000

# Supabase
SUPABASE_URL=https://xxxx.supabase.co
SUPABASE_SERVICE_ROLE_KEY=replace_me
SUPABASE_USERS_TABLE=users
SUPABASE_CHAT_ID_COLUMN=chat_id
SUPABASE_BALANCE_COLUMN=balance
SUPABASE_SOURCE_COLUMN=
AUTO_CREATE_USER=true

# Image generation upstream
LAOZHANG_URL=https://api.laozhang.ai/v1beta/models/gemini-3-pro-image-preview:generateContent
LAOZHANG_API_KEY=replace_me
# bearer | query
LAOZHANG_AUTH_MODE=bearer

